body {
    /*background: #F5F5F5;*/
    height: 100%;
    color: #333;
    font-family: "微软雅黑";
}
@media(max-device-width:320px){
    html{ font-size: 50%;}
}
@media(min-device-width:375px){
    html{font-size:60%;}
}
@media(min-device-width:414px){
    html{font-size:67.5%;}
}
html {
    font-size: 50%;
}

a {
    color: #666 !important;
    font-size: 1em;
}

a:hover {
    text-decoration: none !important;
}

a:focus {
    text-decoration: none !important;
}

.f1 {
    float: left;
}

.f2 {
    float: right;
}

.c1 {
    clear: both;
}

/*******************登录****************/
.main {
    padding-top: 60px;
}

.navigation {
    width: 100%;
    padding-right: 15px;
    padding-left: 15px;
    margin: 0 auto;
    background: #ff5400;
    height: 50px;
    line-height: 50px;
}

.navigation span {
    color: #fff;
    font-size: 1.87rem;
    /*padding-top:2.5%;*/
}

.logo_p1 {
    color: #fff;
    font-size: 1.87rem;
    display: inline-block;
    margin-left: 28%;
    font-weight: bold;
    /*padding-top:2.5%;*/
}

.login_a1 {
    background: #ff5400;
    height: 40px;
    line-height: 40px;
    display: inline-block;
    width: 100%;
    color: #fff !important;
    font-size: 1.87rem;
    border-radius: 5px;
    margin-top: 4%;
    margin-bottom: 1%;
}

.login_p2 {
    margin-top: 10px;
    font-size: 1.5rem;
}

.login_p2 span {
    color: #aaa;
}

.login_p2 span a {
    font-style: normal;
    color: #ff5400 !important;
}

.login_p2 em a {
    font-style: normal;
    color: #ff5400 !important;
}

/*******************注册****************/
.enroll_a1 {
    height: 40px;
    line-height: 40px;
    color: #ff5400 !important;
    text-align: center;
    border: 1px solid #ff5400;
    display: inline-block;
    font-size: 1.5rem;
    z-index: 111;
    border-radius: 5px;
}

.enroll_a1:hover {
    background: #ff5400;
    color: #fff !important;
}

.enroll_p1 {
    font-size: 1.37rem;
    border-left: 5px solid #ff5400;
    padding-left: 5%;
    margin-top: 5%;
    color: #2f2f2f;
}

.enroll_span01 {
    font-size: 1.37rem;
    color: #ff5400;
}

/*footer*/
.footer .message {
    border-top: 1px solid #BFBFBF;
    border-bottom: 1px solid #BFBFBF;
    font-size: 1em;
    color: #666;
    padding-top: 2%;
    padding-bottom: 2%;
    background: #fff;
    margin-top: 5%;
}

.final {
    color: #666;
    font-size: 1em;
    margin-top: 6%;
    text-align: center;
    /*margin-bottom: 5%;*/
}

/*我的银行卡*/
.cardShow_div01 {
    background: #fff;
    border-radius: 20px;
}

.cardShow_p01 {
    font-size: 1.7rem;
    margin: 5% 0;
    border-top: 1px dashed #E8E8E8;
    padding: 10% 5% 15% 5%;
}

.cardShow_img01 {
    width: 50%;
    height: auto;
    margin-left: 3%;
    margin-top: 2%;
}

.cardShow_img02 {
    width: 20%;
    height: auto;
    margin-right: 5%;
    margin-top: 5%;
}

.credentials_p2 {
    border: 1px solid #ff5400;
    padding: 3%;
    border-radius: 15px;
    margin-top: 5%;
    font-size: 1.87rem;
    font-weight: bold;
    text-align: center;
}

.credentials_p2 a {
    color: #ff5400 !important;
}

