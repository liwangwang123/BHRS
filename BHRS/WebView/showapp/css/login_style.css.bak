/*我的订单*/
.order_span01 {
    font-size: 1.5rem;
    margin-right: 3%;
}

.order_span02 {
    font-size: 1.37rem;
    color: #999;
    margin-left: 3%;
}

.order_span03 {
    font-size: 1.5rem;
    color: #ff5400;
}

.order_div01 {
    background: #F5F5F5;
    padding: 5% 2%;
}

.order_img01 {
    width: 28%;
    height: auto;
}

.order_p1 {
    width: 47%;
    display: inline-block;
    margin-left: 2%;
    font-size: 1.37rem;
}

.order_p2 {
    width: 20%;
    font-size: 1.37rem;
    display: inline-block;
    text-align: right;
}

.order_p2 em {
    color: #999;
    font-style: normal;
}

.order_input01 {
    color: #ff5400;
    border: 1px solid #ff5400;
    background: #fff!important;
    margin-top: 3.5%;
    margin-right: 15px;
}



/*李都光*/
.order_span04 {
    font-size: 1.5rem;
    color: #333333;
}
.order_input02 {
    width: 70px;
    color: #fff;
    border: 1px solid #ff5400;
    background: #ff5400!important;
    margin-top: 3.5%;
    margin-right: 15px;
}
/*李都光*/

